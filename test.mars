
attr Complex {
	add(other) => {
		real = self.real + other.real
		imag = self.imag + other.imag
		:: Complex
	}

	to_string() {
		if self.imag < 0 {
			return '#{self.real} - #{-self.imag}i'
		} else {
			return '#{self.real} + #{self.imag}i'
		}
	}
}

let c1 = {
	real = 5
	imag = 3
	:: Complex
}

let c2 = {
	real = 5
	imag = 3
	:: Complex
}

writeline(c1 - c2)
