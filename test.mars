let make_factorial = function () {
	let cache = [1, 1]
	let factorial = function (n) {
		if n < 0 { return 1 }
		if n >= size(cache) {
			cache[n] = n * factorial(n - 1)
		}

		return cache[n]
	}
	return factorial
}

let factorial = make_factorial()

writeline(factorial(9))
writeline(factorial(7))
writeline(factorial(3))
writeline(factorial(1))
writeline(factorial(5))
